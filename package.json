  "dependencies": {
    "@sentry/node": "^7.14.0",
    "@sentry/tracing": "^7.14.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cls-hooked": "^4.2.2",
    "dotenv": "^16.0.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^6.0.0",
