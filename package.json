  "dependencies": {
    "@sentry/node": "^7.14.0",
    "@sentry/tracing": "^7.14.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cls-hooked": "^4.2.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
